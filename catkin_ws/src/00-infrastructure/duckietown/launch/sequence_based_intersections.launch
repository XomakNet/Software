<launch>
    <arg name="veh" doc="Name of vehicle. ex: megaman"/>
    <arg name="local" default="true"
         doc="true for running everything you possibly can on laptop. false for everything running on vehicle."/>
    <arg name="config" default="baseline" doc="Specify a config."/>
    <arg name="param_file_name" default="default" doc="Specify a param file. ex:megaman."/>
    <arg name="live" default="true" doc="if set to true, run the camera and do live detection. "/>

    <include file="$(find duckietown)/launch/cmd_based_intersections.launch">
        <arg name="veh" value="$(arg veh)"/>
        <arg name="local" value="$(arg local)"/>
        <arg name="config" value="$(arg config)"/>
        <arg name="param_file_name" value="$(arg param_file_name)"/>
        <arg name="live" value="$(arg live)"/>
    </include>

    <remap from="tag_sequence_detector_node/tags" to="tag_detections"/>
    <include file="$(find tag_sequence_following)/launch/tag_sequence_detector_node.launch">
        <arg name="veh" value="$(arg veh)"/>
        <arg name="local" value="$(arg local)"/>
        <arg name="config" value="$(arg config)"/>
        <arg name="param_file_name" value="$(arg param_file_name)"/>
    </include>

    <remap from="tag_sequence_controller_node/fsm_state" to="fsm_node/mode"/>
    <remap from="tag_sequence_controller_node/get_current_action" to="tag_sequence_detector_node/get_current_action"/>
    <remap from="tag_sequence_controller_node/turn_left" to="open_loop_intersection_control_node/turn_left"/>
    <remap from="tag_sequence_controller_node/turn_right" to="open_loop_intersection_control_node/turn_right"/>
    <remap from="tag_sequence_controller_node/turn_forward" to="open_loop_intersection_control_node/turn_forward"/>
    <include file="$(find tag_sequence_following)/launch/tag_sequence_controller_node.launch">
        <arg name="veh" value="$(arg veh)"/>
        <arg name="local" value="$(arg local)"/>
        <arg name="config" value="$(arg config)"/>
        <arg name="param_file_name" value="$(arg param_file_name)"/>
    </include>


</launch>
